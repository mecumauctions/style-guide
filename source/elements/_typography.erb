<section role="region" aria-labelledby="typography" class="page__section">
  <header role="banner" class="section__header section__header--alt">
    <div class="header__container">
      <h4 class="header__title--section" id="typography">Typography</h4>
    </div>
  </header>
  <div class="section__body clearfix">
    <header role="title" class="body__header">
      <h5 class="body__header__title">Standard Font Family: Univers</h5>
    </header>
    <% data.elements.font_family.each do |f| %>
      <h3 class="float--left margin-right--echo text--light text--uppercase"><%= f.letter %></h3>
    <% end %>
    <div class="clear">
      <% data.elements.font_family.each do |f| %>
        <h3 class="float--left margin-right--echo text--light"><%= f.letter %></h3>
      <% end %>
    </div>
  </div>
  <div class="section__body">
    <header role="title" class="body__header">
      <h5 class="body__header__title">Home Page</h5>
    </header>
    <div class="event">
      <!-- Image home page -->
      <div class="event event-collector-cars">
        <div class="event-header">
          <a href="#" title="View Chicago 2016 Lots">
            <span class="event-title txt-brand">Chicago</span>
            <span class="event-dates"> 
              <time class="event-start-date" date="2016-10-06">October 6</time>-<time class="event-end-date" datetime="2016-10-08">8</time> <span class="slashes"></span>1,000 VEHICLES
            </span>
          </a>
        </div>
        <div class="event-image-container">
          <a href="#" title="View Chicago 2016 Lots">
            <div class="event-title-container">
              <h2 class="event-title hidden-xs hidden-ms txt-brand txt-lg txt-fw-bold txt-no-shadow">Chicago</h2>
            </div>
            <div class="event-details hidden-xs hidden-ms txt-white txt-lg txt-fw-bold txt-no-shadow">
              <span class="event-dates"> 
                <time class="event-start-date" date="2016-10-06">October 6</time>-<time class="event-end-date" datetime="2016-10-08">8</time>
              </span>
              <span class="event-numbers">1,000 VEHICLES</span> 
              <span class="tv-logo tv-logo-default tv-logo-no-shadow" title="Broadcast Live on NBCSN"></span>
            </div>
            <picture class="event-photo">
            <!--[if IE 9]><video style="display: none;"><![endif]-->
              <source media="(min-width: 768px)" srcset="https://cdn2.mecum.com/assets/images/auctions/ch1016/car_banner_20160919_155014.jpg, https://cdn3.mecum.com/assets/images/auctions/ch1016/car_banner_2x_20160919_155017.jpg 2x">
              <source media="(min-width: 480px)" srcset="https://cdn1.mecum.com/assets/images/auctions/ch1016/car_mobile_banner_2x_20160919_155024.jpg">
              <source srcset="https://cdn3.mecum.com/assets/images/auctions/ch1016/car_mobile_banner_20160919_155021.jpg, https://cdn1.mecum.com/assets/images/auctions/ch1016/car_mobile_banner_2x_20160919_155024.jpg 2x">
            <!--[if IE 9]></video><![endif]-->
              <img src="data:image/gif;base64,R0lGODlhAQABAAAAADs=" alt="Collector and Classic Car Auction - Chicago">
            </picture>
          </a>
        </div>
      </div>
      <header role="title" class="body__header">
        <h5 class="body__header__title">Sample Code</h5>
      </header>
      <% code("html") do %>
<div class="event-title-container">
  <h2 class="event-title hidden-xs hidden-ms txt-brand txt-lg txt-fw-bold txt-no-shadow">H2: Chicago</h2>
</div>
