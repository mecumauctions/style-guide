      <% end %>
      <h4>H4: Featured Chicago Lots</h4>
      <div class="event">
        <div class="event-featured-lots">
          <div class="event-featured-lot col-xs-3">
            <span class="lot-number">Lot S99</span>
            <span class="lot-title">1969 Pontiac GTO Judge </span>
          </div>
        </div>
      </div><br><br>
      <header role="title" class="body__header">
        <h5 class="body__header__title">Sample Code</h5>
      </header>
